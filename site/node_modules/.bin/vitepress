#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/meng/stu/js/Package Manage Tools/Pnpm/Monorepo/vue3-ui/node_modules/.pnpm/vitepress@1.0.0-rc.35_@algolia+client-search@4.22.1_@types+node@20.11.5_less@4.2.0_postcss@8._h5ajyusloingevut5qjvd2nnc4/node_modules/vitepress/bin/node_modules:/Users/meng/stu/js/Package Manage Tools/Pnpm/Monorepo/vue3-ui/node_modules/.pnpm/vitepress@1.0.0-rc.35_@algolia+client-search@4.22.1_@types+node@20.11.5_less@4.2.0_postcss@8._h5ajyusloingevut5qjvd2nnc4/node_modules/vitepress/node_modules:/Users/meng/stu/js/Package Manage Tools/Pnpm/Monorepo/vue3-ui/node_modules/.pnpm/vitepress@1.0.0-rc.35_@algolia+client-search@4.22.1_@types+node@20.11.5_less@4.2.0_postcss@8._h5ajyusloingevut5qjvd2nnc4/node_modules:/Users/meng/stu/js/Package Manage Tools/Pnpm/Monorepo/vue3-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/meng/stu/js/Package Manage Tools/Pnpm/Monorepo/vue3-ui/node_modules/.pnpm/vitepress@1.0.0-rc.35_@algolia+client-search@4.22.1_@types+node@20.11.5_less@4.2.0_postcss@8._h5ajyusloingevut5qjvd2nnc4/node_modules/vitepress/bin/node_modules:/Users/meng/stu/js/Package Manage Tools/Pnpm/Monorepo/vue3-ui/node_modules/.pnpm/vitepress@1.0.0-rc.35_@algolia+client-search@4.22.1_@types+node@20.11.5_less@4.2.0_postcss@8._h5ajyusloingevut5qjvd2nnc4/node_modules/vitepress/node_modules:/Users/meng/stu/js/Package Manage Tools/Pnpm/Monorepo/vue3-ui/node_modules/.pnpm/vitepress@1.0.0-rc.35_@algolia+client-search@4.22.1_@types+node@20.11.5_less@4.2.0_postcss@8._h5ajyusloingevut5qjvd2nnc4/node_modules:/Users/meng/stu/js/Package Manage Tools/Pnpm/Monorepo/vue3-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
